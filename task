print('------9------')
mat_st = np.full((8,8),[0])
mat7 = np.eye(8,k=7)
mat6 = np.eye(8,k=-1)
mat5 = np.eye(8,k=5)
mat4 = np.eye(8,k=-5)
mat3 = np.eye(8,k=3)
mat2 = np.eye(8,k=-7)
mat1 = np.eye(8,k=1)
mat0 = np.eye(8,k=0)
mat = np.eye(8,k=-3)

result = mat_st + mat7 + mat6 + mat5 + mat4 + mat3 + mat2 + mat1 + mat

print(result)

print('------10------')
