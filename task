print('------13------')

Array1 = np.array([10, 20, 30, 40, 50])
Array2 = np.array([10, 40])
Array3 = np.full((np.size(Array1)),Array2[0])
Array4 = np.full((np.size(Array1)),Array2[1])
sup1 = (Array1 == Array3)
sup2 = (Array1 == Array4)
ans = sup1 + sup2
print(ans)

